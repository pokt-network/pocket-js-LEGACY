{
  "name": "@pokt-network/pocket-js-relayer",
  "version": "0.0.1-rc",
  "description": "Pocket-JS Relayer package",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generateDocs": "typedoc",
    "browserify": "browserify browserify.js -p [ minifyify --no-map ] --standalone PocketJSRelayer > dist/web.js",
    "test:integration": "nyc mocha --require ts-node/register \"tests/src/integration/**/*.ts\" --timeout 0 --exit",
    "build": "rm -rf dist && mkdir dist && cti create ./src && npm run lint && tsc && npm run generateDocs",
    "prepare": "npm run build && npm run browserify",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "update": "rm -rf dist && rm -rf node_modules && rm -rf package-lock.json && npm install"
  },
  "keywords": [
    "pocket-js",
    "pocket",
    "network",
    "blockchain",
    "sdk",
    "typescript",
    "javascript",
    "relay",
    "relayer"
  ],
  "author": "Pocket Network",
  "license": "MIT",
  "dependencies": {
    "@pokt-network/aat-js": "^0.1.1-rc",
    "@pokt-network/pocket-js-configuration": "../../sub-modules/configuration",
    "@pokt-network/pocket-js-http-provider": "../../sub-modules/http-provider",
    "@pokt-network/pocket-js-keybase": "../keybase",
    "@pokt-network/pocket-js-nock-utils": "../../sub-modules/nock-utils",
    "@pokt-network/pocket-js-relay-models": "../../sub-modules/relay-models",
    "@pokt-network/pocket-js-rpc-client": "../../sub-modules/rpc-client",
    "@pokt-network/pocket-js-rpc-models": "../../sub-modules/rpc-models",
    "@pokt-network/pocket-js-session-manager": "../session-manager",
    "@pokt-network/pocket-js-storage": "../../sub-modules/storage",
    "@pokt-network/pocket-js-test-utils": "../../sub-modules/test-utils",
    "@pokt-network/pocket-js-utils": "../../sub-modules/utils",
    "browserify": "^17.0.0",
    "minifyify": "^7.3.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.17",
    "chai": "^4.2.0",
    "create-ts-index": "^1.13.6",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "ts-node": "^8.6.2",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.24.0",
    "typedoc": "^0.20.36",
    "typescript": "^3.7.2"
  }
}
